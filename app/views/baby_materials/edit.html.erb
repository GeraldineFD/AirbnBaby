<div class="container">
  <div class="row justify-content-center">
    <div class="col-md-8">
      <h1>Edit baby material</h1>

      <%= simple_form_for @baby_material do |f| %>
        <%= f.input :title %>
        <%= f.input :description %>
        <%= f.input :price,  as: :integer %>

        <%= f.button :submit, class: 'btn btn-primary' %>
      <% end %>
    </div>
  </div>
</div>
